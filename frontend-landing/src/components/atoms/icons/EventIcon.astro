  class={className}
  width="24"
  height="24"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
>
  <path d="M5 8h14"></path>
  <path d="M5 12h14"></path>
  <path d="M5 16h14"></path>
  <path d="M3 20h18"></path>
